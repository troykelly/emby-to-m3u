# Test Environment Variables
# Copy this file to .env.test and fill in your test server details

# Subsonic Test Server
SUBSONIC_HOST=https://your-subsonic-test-server.com
SUBSONIC_USER=test_username
SUBSONIC_PASSWORD=test_password

# AzuraCast Test Server
AZURACAST_HOST=https://your-azuracast-test-server.com
AZURACAST_API_KEY=your_test_api_key_here
AZURACAST_STATION_ID=1

# Feature Flags
AZURACAST_FORCE_REUPLOAD=false
AZURACAST_LEGACY_DETECTION=false
AZURACAST_CACHE_TTL=300
AZURACAST_SKIP_REPLAYGAIN_CHECK=false
