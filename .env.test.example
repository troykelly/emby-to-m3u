# Test Environment Configuration Template
# Copy this to .env.test and fill in actual values for integration testing

# Subsonic Server Configuration
SUBSONIC_URL=https://your-subsonic-server.com
SUBSONIC_USER=testuser
SUBSONIC_PASSWORD=testpassword

# AzuraCast Server Configuration
AZURACAST_HOST=https://your-azuracast-server.com
AZURACAST_API_KEY=your-api-key-here
AZURACAST_STATIONID=1

# AzuraCast Duplicate Detection Configuration
AZURACAST_FORCE_REUPLOAD=false
AZURACAST_LEGACY_DETECTION=false
AZURACAST_CACHE_TTL=300
AZURACAST_SKIP_REPLAYGAIN_CHECK=false
